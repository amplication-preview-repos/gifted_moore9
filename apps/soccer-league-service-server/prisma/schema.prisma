datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Team {
  country     String?
  createdAt   DateTime @default(now())
  foundedYear Int?
  id          String   @id @default(cuid())
  name        String?
  stadium     String?
  updatedAt   DateTime @updatedAt
}
